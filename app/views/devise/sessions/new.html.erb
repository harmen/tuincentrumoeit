<body class="loginpage">

    <div id="navbar">
        <a class="active" href="/">Home</a>
        <%= link_to products_url do %>Product overzicht</a><% end %>
        <%= link_to dashboard_products_url do %>Dashboard<% end %>
    </div>

    <div class="container">
        <div class="row">
            <div class="col-lg">
                <div class="login-content">
                    <br>
                    <h1>Login</h1>
                    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                    <div class="form-group">
                        <%= f.email_field :email, class: "form-control", placeholder: "Email", autocomplete: "email", id: "exampleInputName1" %>
                    </div>
                    <div class="form-group">
                        <%= f.password_field :password, class: "form-control", id: "exampleInputPassword1", placeholder: "Password" %>
                    </div>

                    <%= f.submit "Log in", class: "btn btn-primary" %>
                    <% end %>
                    <%= render "devise/shared/links" %>
                </div>
            </div>
        </div>
    </div>